<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.unifacs.a1">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.A1">

        <activity
            android:name=".NavegacaoActivity"
            android:exported="true"
            android:label="@string/labelNavegacao" />
        <activity
            android:name=".HistoricoActivity"
            android:exported="true"
            android:label="@string/labelHistorico" />
        <activity
            android:name=".GNSSActivity"
            android:exported="true"
            android:label="GNSS" />
        <activity
            android:name=".CreditActivity"
            android:exported="true"
            android:label="@string/labelCreditos" />
        <activity
            android:name=".ConfigActivity"
            android:exported="true"
            android:label="@string/labelConfig" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/labelInicio">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>